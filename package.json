    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "lucide-react": "^0.344.0",
    "mime-types": "^2.1.35",
    "openai": "^4.77.0",
